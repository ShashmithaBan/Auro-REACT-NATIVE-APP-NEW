{
  "expo": {
    "name": "auro",
    "slug": "auro",
     "scheme": "auro",
    "version": "1.0.0",
    "orientation": "portrait",
 

    "userInterfaceStyle": "automatic",
    "splash": {
   
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
    
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
   
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
